issn = {0031899X},
journal = {Phys. Rev.},
number = {2},
pages = {487--494},
title = {{Crystal dynamics of potassium. I. Pseudopotential analysis of phonon dispersion curves at 9°K}},
volume = {150},
year = {1966}
}
