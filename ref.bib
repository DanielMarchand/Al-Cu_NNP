pages = {1--21},
title = {{Magnon and Phonon Dispersion, Lifetime and Thermal Conductivity of Iron from Spin-Lattice Dynamics Simulations}},
url = {https://arxiv.org/ftp/arxiv/papers/1712/1712.09002.pdf}
}
