doi = {10.1063/1.2966006},
file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Ram{\'{i}}rez et al. - 2008 - Path integral calculation of free energies Quantum effects on the melting temperature of neon.pdf:pdf},
