isbn = {1359-6454},
issn = {13596454},
journal = {Acta Mater.},
keywords = {Aluminum alloys,Natural aging,Phase transformation kinetics,Trace elements,Vacancies},
pages = {296--305},
publisher = {Elsevier Ltd},
title = {{Design strategy for controlled natural aging in Al–Mg–Si alloys}},
url = {http://dx.doi.org/10.1016/j.actamat.2016.07.048},
volume = {118},
year = {2016}
}
