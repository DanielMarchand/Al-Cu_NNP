@article{Turney2009,
abstract = {Two methods for predicting phonon frequencies and relaxation times$\backslash$nare presented. The first is based on quasiharmonic and anharmonic$\backslash$nlattice dynamics calculations, and the second is based on a combination$\backslash$nof quasiharmonic lattice dynamics calculations and molecular dynamics$\backslash$nsimulations. These phonon properties are then used with the Boltzmann$\backslash$ntransport equation under the relaxation-time approximation to predict$\backslash$nthe lattice thermal conductivity. The validity of the low-temperature$\backslash$nassumptions made in the lattice dynamics framework are assessed by$\backslash$ncomparing to thermal conductivities predicted by the Green-Kubo and$\backslash$ndirect molecular dynamics methods for a test system of Lennard-Jones$\backslash$nargon. The predictions of all four methods are in agreement at low$\backslash$ntemperature (20 K). At temperatures of 40 K (half the Debye temperature$\backslash$nof Lennard-Jones argon) and below, the thermal-conductivity predictions$\backslash$nfrom the two methods that use lattice dynamics calculations are within$\backslash$nabout 30{\%} of those made using the more accurate Green-Kubo and direct$\backslash$nmolecular dynamics methods. The thermal-conductivity predictions$\backslash$nusing the lattice dynamics techniques become inaccurate at high temperature$\backslash$n(above 40 K) due to the approximations inherent in the lattice dynamics$\backslash$nframework. We apply the results to assess the validity of (i) the$\backslash$nisotropic approximation in modeling thermal transport and (ii) the$\backslash$ncommon assertion that low-frequency phonons dominate thermal transport.$\backslash$nLastly, we suggest approximations that can be made within the lattice$\backslash$ndynamics framework that allow the thermal conductivity of Lennard-Jones$\backslash$nargon to be estimated using two orders of magnitude less computing$\backslash$neffort than the Green-Kubo or direct molecular dynamics methods.},
author = {Turney, J. E. and Landry, E. S. and McGaughey, a. J H and Amon, C. H.},
doi = {10.1103/PhysRevB.79.064301},
file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Turney et al. - 2009 - Predicting phonon properties and thermal conductivity from anharmonic lattice dynamics calculations and molecular.pdf:pdf},
isbn = {1098-0121},
issn = {1098-0121},
journal = {Phys. Rev. B},
keywords = {anharmonic,thermal conductivity},
mendeley-tags = {anharmonic,thermal conductivity},
month = {feb},
number = {6},
pages = {064301},
title = {{Predicting phonon properties and thermal conductivity from anharmonic lattice dynamics calculations and molecular dynamics simulations}},
