issn = {0953-8984},
journal = {J. Phys. Condens. Matter},
keywords = {225402,26,anharmonic force constant,available from stacks,boltzmann transport equation,dynamics,first-principles calculation,in colour only in,iop,j,lattice thermal conductivity,mg2si,mmedia,non-equilibrium molecular,org,physcm,s online supplementary data,si,some figures may appear,the online journal,thermal conductivity},
mendeley-tags = {mg2si,si,thermal conductivity},
month = {jun},
number = {22},
