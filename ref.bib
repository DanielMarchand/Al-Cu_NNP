file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Bo{\v{z}}in et al. - 2010 - Entropically stabilized local dipole formation in lead chalcogenides.pdf:pdf},
issn = {1095-9203},
journal = {Science},
month = {dec},
