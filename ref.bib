journal = {Phys. Rev. B - Condens. Matter Mater. Phys.},
keywords = {phonon-life-time},
mendeley-tags = {phonon-life-time},
number = {8},
pages = {1--11},
title = {{Heat transport in silicon from first-principles calculations}},
