issn = {13596454},
journal = {Acta Mater.},
keywords = {6061,Al-Mg-Si,Aluminum alloys,Atom probe tomography,Metastable phases,Transmission electron microscopy},
mendeley-tags = {Al-Mg-Si},
pages = {209--221},
publisher = {Elsevier Ltd},
title = {{Analysis of the metastable precipitates in peak-hardness aged Al-Mg-Si(-Cu) alloys with differing Si contents}},
