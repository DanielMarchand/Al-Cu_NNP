journal = {J. Chem. Phys.},
keywords = {Al-Mg-Si/,NN,machine-learning},
mendeley-tags = {Al-Mg-Si/,NN,machine-learning},
number = {24},
title = {{Automatic selection of atomic fingerprints and reference configurations for machine-learning potentials}},
volume = {148},
