file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Gadway et al. - 2013 - Evidence for a quantum-to-classical transition in a pair of coupled quantum rotors.pdf:pdf},
issn = {00319007},
journal = {Phys. Rev. Lett.},
number = {MAY},
pages = {1--5},
title = {{Evidence for a quantum-to-classical transition in a pair of coupled quantum rotors}},
volume = {110},
year = {2013}
}
