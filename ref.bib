file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Clouet, Nastar, Sigli - 2004 - Nucleation of Al3Zr and Al3Sc in aluminum alloys From kinetic Monte Carlo simulations to classical theory.pdf:pdf},
isbn = {1098-0121},
issn = {1550235X},
journal = {Phys. Rev. B - Condens. Matter Mater. Phys.},
keywords = {KMC},
mendeley-tags = {KMC},
number = {6},
pages = {1--14},
primaryClass = {cond-mat},
title = {{Nucleation of Al3Zr and Al3Sc in aluminum alloys: From kinetic Monte Carlo simulations to classical theory}},
volume = {69},
year = {2004}
}
@article{Errea2016,
abstract = {The quantum nature of the proton can crucially affect the structural and physical properties of hydrogen compounds. For example, in the high-pressure phases of H2O, quantum proton fluctuations lead to symmetrization of the hydrogen bond and reduce the boundary between asymmetric and symmetric structures in the phase diagram by 30 gigapascals (ref. 3). Here we show that an analogous quantum symmetrization occurs in the recently discovered sulfur hydride superconductor with a superconducting transition temperature Tc of 203 kelvin at 155 gigapascals-the highest Tc reported for any superconductor so far. Superconductivity occurs via the formation of a compound with chemical formula H3S (sulfur trihydride) with sulfur atoms arranged on a body-centred cubic lattice. If the hydrogen atoms are treated as classical particles, then for pressures greater than about 175 gigapascals they are predicted to sit exactly halfway between two sulfur atoms in a structure with symmetry. At lower pressures, the hydrogen atoms move to an off-centre position, forming a short H-S covalent bond and a longer H{\textperiodcentered}{\textperiodcentered}{\textperiodcentered}S hydrogen bond in a structure with R3m symmetry. X-ray diffraction experiments confirm the H3S stoichiometry and the sulfur lattice sites, but were unable to discriminate between the two phases. Ab initio density-functional-theory calculations show that quantum nuclear motion lowers the symmetrization pressure by 72 gigapascals for H3S and by 60 gigapascals for D3S. Consequently, we predict that the phase dominates the pressure range within which the high Tc was measured. The observed pressure dependence of Tc is accurately reproduced in our calculations for the phase, but not for the R3m phase. Therefore, the quantum nature of the proton fundamentally changes the superconducting phase diagram of H3S.},
author = {Errea, Ion and Calandra, Matteo and Pickard, Chris J. and Nelson, Joseph R. and Needs, Richard J. and Li, Yinwei and Liu, Hanyu and Zhang, Yunwei and Ma, Yanming and Mauri, Francesco},
doi = {10.1038/nature17175},
file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Errea et al. - 2016 - Quantum hydrogen-bond symmetrization in the superconducting hydrogen sulfide system.pdf:pdf},
issn = {0028-0836},
journal = {Nature},
number = {7597},
pages = {81--84},
pmid = {27018657},
publisher = {Nature Publishing Group},
title = {{Quantum hydrogen-bond symmetrization in the superconducting hydrogen sulfide system}},
url = {http://www.nature.com/doifinder/10.1038/nature17175},
volume = {532},
year = {2016}
}
