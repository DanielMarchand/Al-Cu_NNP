file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Dragoni et al. - 2018 - Achieving DFT accuracy with a machine-learning interatomic potential Thermomechanics and defects in bcc ferro(2).pdf:pdf},
issn = {2475-9953},
journal = {Phys. Rev. Mater.},
keywords = {doi:10.1103/PhysRevMaterials.2.013808 url:https://},
month = {jan},
