@article{Cepellotti2016a,
abstract = {Thermal conductivity in dielectric crystals is the result of the non-equilibrium relaxation of lattice vibrations described by the phonon Boltzmann transport equation. An exact microscopic definition of the heat carriers and their relaxation times is still missing: phonons, typically regarded as the relevant excitations for thermal transport, cannot be identified as the heat carriers when most scattering events conserve momentum and do not dissipate heat. This is the case for two-dimensional or layered materials at room temperature, or three-dimensional crystals at cryogenic temperature. In this work we show first that the eigenvectors of the scattering matrix in the Boltzmann equation define collective phonon excitations, termed here relaxons. These excitations have well defined relaxation times directly related to heat dissipation, and provide an exact description of thermal transport as a kinetic theory of the relaxon gas. We show why Matthiessen's rule is typically violated, and construct a procedure for obtaining the mean free paths and relaxation times of the relaxons. Such considerations, while general, are of the utmost importance in the case of two-dimensional materials and revise by several orders of magnitude the time-scales (and, in part, the length-scales) for thermal transport.},
archivePrefix = {arXiv},
arxivId = {1603.02608},
author = {Cepellotti, Andrea and Marzari, Nicola},
doi = {10.1103/PhysRevX.6.041013},
eprint = {1603.02608},
file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Cepellotti, Marzari - 2016 - Thermal transport in crystals as a kinetic theory of relaxons.pdf:pdf},
issn = {2160-3308},
keywords = {computational physics,condensed matter physics},
pages = {1--19},
title = {{Thermal transport in crystals as a kinetic theory of relaxons}},
url = {http://arxiv.org/abs/1603.02608},
volume = {041013},
year = {2016}
}
