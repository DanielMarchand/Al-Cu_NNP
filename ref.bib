journal = {J. Chem. Phys.},
number = {3},
pages = {034109},
pmid = {20649310},
title = {{First-principles theories for anharmonic lattice vibrations.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20649310},
volume = {133},
year = {2010}
}
