journal = {Mater. Sci. Eng. A},
keywords = {Al-Mg-Si,Aluminum alloys,Diffusion modeling,Impurity diffusion coefficient,Semi-empirical correlations,diffusion},
mendeley-tags = {Al-Mg-Si,diffusion},
number = {1-2},
