issn = {00104655},
journal = {Comput. Phys. Commun.},
keywords = {Boltzmann transport equation,Phonon,Thermal conductivity},
month = {nov},
pages = {351--362},
title = {{almaBTE : A solver of the space–time dependent Boltzmann transport equation for phonons in structured materials}},
