@article{Kresch2007,
abstract = {Measurements of elastic and inelastic neutron scatterings from elemental nickel were made at 10, 300, 575, 875, and 1275 K. The phonon densities of states (DOSs) were calculated from the inelastic scattering and were fit with Born–von K{\'{a}}rm{\'{a}}n models of the lattice dynamics. With ancillary data on thermal expansion and elastic moduli, we found a small, negative anharmonic contribution to the phonon entropy at high temperature. We used this to place bounds on the magnetic entropy of nickel. A significant broadening of the phonon DOS at elevated temperatures, another indication of anharmonicity, was also measured and quantified.},
