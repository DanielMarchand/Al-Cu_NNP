isbn = {2160-3308},
issn = {21603308},
journal = {Phys. Rev. X},
keywords = {Computational physics,Condensed matter physics,Materials science},
number = {1},
pages = {1--9},
title = {{Finding unprecedentedly low-thermal-conductivity half-heusler semiconductors via high-throughput materials modeling}},
volume = {4},
year = {2014}
}
