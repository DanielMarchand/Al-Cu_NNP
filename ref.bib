keywords = {Coating,Molecular Dynamics,Solidification,Structural zone,Thermal spray},
pages = {72--80},
publisher = {Elsevier B.V.},
title = {{Large scale Molecular Dynamics simulation of microstructure formation during thermal spraying of pure copper}},
url = {http://dx.doi.org/10.1016/j.surfcoat.2015.08.034},
volume = {280},
year = {2015}
}
