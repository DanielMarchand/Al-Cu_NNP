@article{Carrete2014a,
abstract = {The lattice thermal conductivity ({\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}}) is a key property for many potential applications of compounds. Discovery of materials with very low or high {\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}} remains an experimental challenge due to high costs and time-consuming synthesis procedures. High-throughput computational pre-screening is a valuable approach for significantly reducing the set of candidate compounds. In this article, we introduce efficient methods for reliably estimating the bulk {\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}} for a large number of compounds. The algorithms are based on a combination of machine-learning algorithms, physical insights, and automatic ab-initio calculations. We scanned approximately 79,000 half-Heusler entries in the AFLOWLIB.org database. Among the 450 mechanically stable ordered semiconductors identified, we find that {\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}} spans more than two orders of magnitude- a much larger range than that previously thought. {\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}} is lowest for compounds whose elements in equivalent positions have large atomic radii. We then perform a thorough screening of thermodynamical stability that allows to reduce the list to 77 systems. We can then provide a quantitative estimate of {\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}} for this selected range of systems. Three semiconductors having {\{}$\backslash$kappa{\}}{\{}$\backslash$omega{\}} {\textless} 5 W /(m K) are proposed for further experimental study.},
archivePrefix = {arXiv},
arxivId = {1401.2439},
author = {Carrete, Jes??s and Li, Wu and Mingo, Natalio and Wang, Shidong and Curtarolo, Stefano},
doi = {10.1103/PhysRevX.4.011019},
