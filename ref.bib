@article{Pham2011,
abstract = {Though not very often, there are some cases in the literature where$\backslash$ndiscrepancies exist in the temperature dependence of elastic constants$\backslash$nof materials. A particular example of this case is the behavior of$\backslash$nC-12 coefficient of a simple metal, aluminum. In this paper we attempt$\backslash$nto provide insight into various contributions to temperature dependence$\backslash$nin elastic properties by investigating the thermoelastic properties$\backslash$nof fcc aluminum as a function of temperature through the use of two$\backslash$ncomputational techniques and experiments. First, ab initio calculations$\backslash$nbased on density functional theory (DFT) are used in combination$\backslash$nwith quasiharmonic theory to calculate the elastic constants at finite$\backslash$ntemperatures through a strain-free energy approach. Molecular dynamics$\backslash$n(MD) calculations using tight-binding potentials are then used to$\backslash$nextract the elastic constants through a fluctuation-based formalism.$\backslash$nThrough this dynamic approach, the different contributions (Born,$\backslash$nkinetic, and stress fluctuations) to the elastic constants are isolated$\backslash$nand the underlying physical basis for the observed thermally induced$\backslash$nsoftening is elucidated. The two approaches are then used to shed$\backslash$nlight on the relatively large discrepancies in the reported temperature$\backslash$ndependence of the elastic constants of fcc aluminum. Finally, the$\backslash$npolycrystalline elastic constants (and their temperature dependence)$\backslash$nof fcc aluminum are determined using resonant ultrasound spectroscopy$\backslash$n(RUS) and compared to previously published data as well as the atomistic$\backslash$ncalculations performed in this work.},
author = {Pham, Hieu H. and Williams, Michael E. and Mahaffey, Patrick and Radovic, Miladin and Arroyave, Raymundo and Cagin, Tahir},
doi = {10.1103/PhysRevB.84.064101},
file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Pham et al. - 2011 - Finite-temperature elasticity of fcc Al Atomistic simulations and ultrasonic measurements.pdf:pdf},
isbn = {1098-0121},
issn = {10980121},
journal = {Phys. Rev. B - Condens. Matter Mater. Phys.},
number = {6},
pages = {1--10},
title = {{Finite-temperature elasticity of fcc Al: Atomistic simulations and ultrasonic measurements}},
volume = {84},
year = {2011}
}
