@article{Ozolins1997,
abstract = {Epitaxial strain energies of epitaxial films and bulk superlattices are studied via first-principles total energy calculations using the local-density approximation. Anharmonic effects due to large lattice mismatch, beyond the reach of the harmonic elasticity theory, are found to be very important in Cu/Au (lattice mismatch 12{\%}), Cu/Ag (12{\%}) and Ni/Au (15{\%}). We find that {\textless}001{\textgreater} is the elastically soft direction for biaxial expansion of Cu and Ni, but it is {\textless}201{\textgreater} for large biaxial compression of Cu, Ag, and Au. The stability of superlattices is discussed in terms of the coherency strain and interfacial energies. We find that in phase-separating systems such as Cu-Ag the superlattice formation energies decrease with superlattice period, and the interfacial energy is positive. Superlattices are formed easiest on (001) and hardest on (111) substrates. For ordering systems, such as Cu-Au and Ag-Au, the formation energy of superlattices increases with period, and interfacial energies are negative. These superlattices are formed easiest on (001) or (110) and hardest on (111) substrates. For Ni-Au we find a hybrid behavior: superlattices along {\textless}111{\textgreater} and {\textless}001{\textgreater} like in phase-separating systems, while for {\textless}110{\textgreater} they behave like in ordering systems. Finally, recent experimental results on epitaxial stabilization of disordered Ni-Au and Cu-Ag alloys, immiscible in the bulk form, are explained in terms of destabilization of the phase separated state due to lattice mismatch between the substrate and constituents.},
archivePrefix = {arXiv},
arxivId = {cond-mat/9711183},
author = {Ozolins, V. and Wolverton, C. and Zunger, Alex},
doi = {10.1103/PhysRevB.57.4816},
eprint = {9711183},
file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Ozolins, Wolverton, Zunger - 1997 - Effects of anharmonic strain on phase stability of epitaxial films and superlattices applications to.pdf:pdf},
isbn = {1098-0121},
issn = {0163-1829},
number = {8},
pages = {16},
primaryClass = {cond-mat},
title = {{Effects of anharmonic strain on phase stability of epitaxial films and superlattices: applications to noble metals}},
url = {http://arxiv.org/abs/cond-mat/9711183},
volume = {57},
year = {1997}
}
