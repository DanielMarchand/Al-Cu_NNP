issn = {09669795},
journal = {Intermetallics},
keywords = {Cyclic deformation twin,Deformation behavior,High entropy alloy,High-cycle fatigue,Tensile},
number = {April},
pages = {106486},
publisher = {Elsevier},
title = {{High-cycle fatigue and tensile deformation behaviors of coarse-grained equiatomic CoCrFeMnNi high entropy alloy and unexpected hardening behavior during cyclic loading}},
url = {https://doi.org/10.1016/j.intermet.2019.106486},
volume = {111},
