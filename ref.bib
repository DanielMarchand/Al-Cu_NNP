file = {:Users/glensk/Library/Application Support/Mendeley Desktop/Downloaded/Kreis et al. - 2015 - Hamiltonian coupling of classical and Path Integral models of atoms.pdf:pdf},
pages = {1--8},
title = {{Hamiltonian coupling of classical and Path Integral models of atoms}},
year = {2015}
}
